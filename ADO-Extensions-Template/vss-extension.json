{
  "manifestVersion": 1,
  "version": "1.0.5",
  "name": "ADO Base Template Extension",
  "description": "My base template extension",
  "publisher": "RazType",
  "demands": ["api-version/3.0"],
  "categories": ["Azure Repos"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "Marketplace.md"
    }
  },
  "icons": {
    "default": "img/logo.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "my-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "My hub group",
      "targets": ["ms.vss-web.project-hub-groups-collection"],
      "properties": {
        "name": "My Hub Group",
        "icon": {
          "dark": "img/sample.png",
          "light": "img/sample.png"
        }
      }
    },
    {
      "id": "hub-hotreload",
      "type": "ms.vss-web.hub",
      "targets": [".my-hub-group"],
      "properties": {
        "uri": "dist/hub-hotreload/hub-hotreload.html",
        "name": "Hot Reload",
        "icon": {
          "dark": "img/world.png",
          "light": "img/world.png"
        }
      }
    },
    {
      "id": "hub-iframe",
      "type": "ms.vss-web.hub",
      "targets": [".my-hub-group"],
      "properties": {
        "uri": "dist/hub-iframe/hub-iframe.html",
        "name": "iFrame",
        "icon": {
          "dark": "img/iframe.png",
          "light": "img/iframe.png"
        }
      }
    }
  ]
}
