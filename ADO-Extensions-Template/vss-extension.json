{
  "manifestVersion": 1,
  "version": "1.0.9",
  "name": "ADO Base Template Extension",
  "description": "My base template extension",
  "publisher": "RazType",
  "demands": ["api-version/3.0"],
  "categories": ["Azure Repos"],
  "tags": [
    "sample",
    "learning"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/raffertyuy/Azure-DevOps-GitHub-Integration-Toolkit"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "Marketplace.md"
    }
  },
  "icons": {
    "default": "img/logo.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    }
  ]
}
