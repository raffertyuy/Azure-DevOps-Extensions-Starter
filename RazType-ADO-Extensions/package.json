{
  "name": "azure-devops-extension-sample",
  "version": "1.0.4",
  "description": "Sample Azure DevOps web extension",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/azure-devops-extension-sample.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/MyExtensions/**/*.json --output-path packages",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/MyExtensions/**/*.json --output-path packages"
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.164.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "sass": "^1.49.9",
    "rimraf": "~2.6.2",
    "sass-loader": "^13.2.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.11.0",
    "ts-loader": "~5.2.2",
    "typescript": "^3.9.6",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  }
}
